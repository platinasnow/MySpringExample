<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="512" maxSessions="512"
		maxTransactions="512" useStatementNamespaces="true" />

	<sqlMap resource="phase/sql/login.xml" />
	<sqlMap resource="phase/sql/admin.xml" />
	<sqlMap resource="phase/sql/board.xml" />
</sqlMapConfig>
